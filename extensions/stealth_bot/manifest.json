{
  "manifest_version": 2,
  "name": "WD_Deletion",
  "version": "1.0",
  "description": "Deletes the webdriver artefacts that reveals a web bot",
  "content_scripts": [
    {
	    "all_frames": true,
      "matches": ["<all_urls>"],
      "js": ["delete_wd.js"],
      "run_at": "document_start"
    }
  ]
}

